#include<iostream>
using namespace std;
int floorsqrt (int x){
	if (x==0 || x==1){
		return x;
	}
	int i  = 1;
	while(1){
                if (i * i > x) {
                  return i - 1;
                }
                i++;
        }
}

        int main() {
                int n;
                cin >> n;
                cout << floorsqrt(n) << endl;
        }
