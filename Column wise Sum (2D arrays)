#include<iostream>
using namespace std;

int main(){
int row,col;
cin>>row>>col;
int mat [row][col];

for(int i  = 0; i <row;i++){
  for(int j =0; j<col; j++){
    cin>>mat[i][j];
  }
}
for(int j = 0; j<col; j++){
 int sum = 0;
  for(int i = 0; i<row; i++){
sum =sum+mat[i][j];
  }
  cout<<sum<<" ";
}
  
}


